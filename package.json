{
  "name": "webpack-project",
  "version": "1.0.0",
  "description": "default webpack project",
  "private": true,
  "scripts": {
    "codestyle": "eslint projects",
    "codestyle:fix": "eslint projects --fix",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "build:production": "cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "start": "cross-env NODE_ENV=development webpack serve --mode development",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "browserslist": "> 0.25%, not dead",
  "keywords": [
    "vue",
    "pug",
    "postcss",
    "svg",
    "eslint",
    "prettier",
    "babel",
    "javascript",
    "webpack"
  ],
  "author": "Bogdan Sutuzhko <julfy.web@gmail.com>",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.[jt]s": [
      "eslint --cache --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "csv-loader": "^3.0.3",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "mini-css-extract-plugin": "^1.2.1",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "papaparse": "^5.3.0",
    "postcss": "^8.1.4",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-inline-svg": "^5.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-nested": "^5.0.1",
    "postcss-pxtorem": "^5.1.1",
    "postcss-rgb": "^1.0.0",
    "postcss-scss": "^3.0.2",
    "postcss-svgo": "^4.0.2",
    "prettier": "^2.1.2",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^5.0.0",
    "svg-transform-loader": "^2.0.13",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^5.0.3",
    "vue-loader": "^15.9.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "normalize.css": "^8.0.1",
    "pug": "^3.0.0",
    "vue": "^2.6.12",
    "vue-router": "^3.4.8",
    "vuex": "^3.5.1"
  },
  "repository": "https://github.com/julfy-bs/webpack-project.git"
}
